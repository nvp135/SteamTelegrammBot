<Window x:Class="TelegramBot.SignInSteam"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:TelegramBot"
      mc:Ignorable="d" 
      Title="SignInSteam" Height="500" Width="700" Initialized="Window_Initialized">

    <Grid>
        <WebBrowser Name="wbAuth" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Source="{x:Null}" LoadCompleted="wbAuth_LoadCompleted" />
    </Grid>
</Window>
